<%= button_to "Back To Profile", profile_path(@current_profile), method: "get" %>
<br>

<h1>All Movies</h1>
<div class="container-sm">
  <div class="row">
    <% @movies.each do |movie| %> 
      <% @parse = movie %>
      <%= render template: "layouts/movie_card" %>
      <% end %>
  </div>
</div>
